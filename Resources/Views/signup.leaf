#embed("header")
<body>
<div class="container mx-auto">
    <h1>Creazione utente</h1>
    <form action="/signup" method="POST">
        <input name="username" type="input" id="username"><br>
        <input name="password" type="password" id="password"><br>
        <select name="type" id="type">
            <option value="aministratore">aministratore</option>
            <option value="punto-vendita">punto-vendita</option>
            <option value="vendita">vendita</option>
            <option value="laboratorio">laboratorio</option>
        </select>
        <input value="Crea Utente" type="submit">
    </form>

<form action="/deleteUsers" method="POST">
    <input value="Elimina utenti" type="submit">
</form>

</div>
</body
<html>
